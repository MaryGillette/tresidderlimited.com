html, body {
	height: 100%;
}

#container {
	position: relative;
	min-height: 100%;
	height: 100%;
	voice-family: "\"}\"";
	voice-family: inherit;
	height: auto;
}
html>body #container {
	height: auto;
}
#content {
	padding: 10px;
	background-color: #a6c;
	padding-bottom: 48px;
}
#footer {
	position: absolute;
	bottom: 0;
	padding: 10px;
	background-color: #609;
	width: 75%;
}
#footer h1 {
	color: #fff;
	padding-bottom: 0;
}
h1, p {
	margin: 0;
	padding-bottom: 1em;
}
h1 {
	font-size: 12px;
	line-height: 1.5em;
}
