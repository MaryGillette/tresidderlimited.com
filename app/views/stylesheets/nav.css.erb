/* =Main Nav
-----------------------------------------------------------------------------*/
#nav_main ul{
	position:absolute;
	right:0;
	top:0;
	margin:0;
	padding:0;
}

#nav_main li{
	float:left;
}

#nav_main li{
	margin-right:30px;
}
#nav_main li#last{
	margin-right:0;
}
#nav_main li, #nav_main li a{
	font:1em verdana;
	text-decoration:none;
	color:#102f84;
	text-transform:lowercase;
}
#nav_main li a{
	display:block;
	font-weight:normal;
	border-bottom:1px solid <%= @dark_blue %>;
}


